# AWS API Configuration
# Copy this file to .env and fill in your actual values

# Classical Serverless API
CLASSICAL_API_URL=https://your-classical-api-id.execute-api.us-east-1.amazonaws.com/demo
CLASSICAL_API_KEY=your-classical-api-key-here

# Quantum Serverless API  
QUANTUM_API_URL=https://your-quantum-api-id.execute-api.us-east-1.amazonaws.com/demo
QUANTUM_API_KEY=your-quantum-api-key-here

# Test Configuration
DEFAULT_TIMEOUT=30
QUANTUM_TIMEOUT=300
TEST_DELAY_SECONDS=2
SCALING_TEST_DELAY=10

# Visualization Settings
SAVE_PLOTS=true
PLOT_DPI=300
PLOT_FORMAT=png

# Logging
LOG_LEVEL=INFO
LOG_FILE=comparison_tests.log

# Optional: AWS Credentials (if not using AWS CLI)
# AWS_ACCESS_KEY_ID=your-access-key
# AWS_SECRET_ACCESS_KEY=your-secret-key
# AWS_DEFAULT_REGION=us-east-1
